{
  "kind": "collectionType",
  "collectionName": "tasks",
  "info": {
    "name": "task"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "keywords": {
      "type": "text"
    },
    "is_finished": {
      "type": "boolean",
      "default": false
    },
    "articles": {
      "via": "task",
      "collection": "article"
    },
    "domain": {
      "type": "string"
    },
    "is_processing": {
      "type": "boolean",
      "default": false
    },
    "is_started": {
      "type": "boolean",
      "default": false
    },
    "is_enabled": {
      "type": "boolean",
      "default": false
    },
    "parser_type": {
      "type": "enumeration",
      "enum": [
        "standard",
        "download"
      ]
    },
    "lang": {
      "type": "enumeration",
      "enum": [
        "ru",
        "en",
        "es"
      ],
      "default": "ru"
    },
    "wp_user": {
      "type": "string"
    },
    "wp_password": {
      "type": "string"
    },
    "server_domain": {
      "type": "string"
    },
    "ssh_user": {
      "type": "string"
    },
    "ssh_password": {
      "type": "string"
    }
  }
}
