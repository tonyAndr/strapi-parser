{
  "kind": "collectionType",
  "collectionName": "articles",
  "info": {
    "name": "article"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "keyword": {
      "type": "string"
    },
    "title_h1": {
      "type": "string"
    },
    "text_length": {
      "type": "integer"
    },
    "seo_meta": {
      "type": "component",
      "repeatable": true,
      "component": "seo.seo-fields"
    },
    "task": {
      "via": "articles",
      "model": "task"
    },
    "is_done": {
      "type": "boolean",
      "default": false
    },
    "is_uploaded": {
      "type": "boolean",
      "default": false
    },
    "content_body": {
      "type": "text"
    }
  }
}
